    <fragment
        android:id="@+id/viewProfiles"
        android:name="com.example.trainingcourse_application.admin.viewprofiles.ViewProfilesFragment"
        android:label="@string/menu_view_profiles"
        tools:layout="@layout/fragment_viewprofiles" />

    <fragment
        android:id="@+id/historyCourse"
        android:name="com.example.trainingcourse_application.admin.historyofcourse.HistoryOfCourseFragment"
        android:label="@string/menu_history_course"
        tools:layout="@layout/fragment_historycourse" />

    <fragment
        android:id="@+id/logout"
        android:name="com.example.trainingcourse_application.shared.logout.LogoutFragment"
        android:label="@string/menu_logout"
        tools:layout="@layout/fragment_logout" />

