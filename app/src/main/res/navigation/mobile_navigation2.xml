<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation2"
    app:startDestination="@+id/home">

    <fragment
        android:id="@+id/home"
        android:name="com.example.trainingcourse_application.shared.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/createCourse"
        android:name="com.example.trainingcourse_application.admin.createcourse.CreateCourseFragment"
        android:label="@string/menu_create_course"
        tools:layout="@layout/fragment_createcourse" />

    <fragment
        android:id="@+id/editDeleteCourse"
        android:name="com.example.trainingcourse_application.admin.editdeletecourse.EditDeleteCourseFragment"
        android:label="@string/menu_edit_delete_course"
